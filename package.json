{
  "name": "@supermdguy/tuxi",
  "repository": {
    "type": "git",
    "url": "https://github.com/superMDguy/tuxi"
  },
  "version": "0.2.0",
  "description": "Automatically manage state of asynchronous tasks.",
  "main": "dist/bundle.umd.js",
  "module": "dist/bundle.es.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest tests/*.test.js",
    "test:ci": "npm test -- --ci --reporters=default --reporters=jest-junit && npx codecov",
    "lint": "eslint --fix ./{lib,tests}/**/*.js",
    "prepublish": "npm run build",
    "build": "npx rollup -c"
  },
  "keywords": [
    "vuex",
    "async",
    "state",
    "vue"
  ],
  "author": "Matthew Dangerfield",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.20.2",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.4.2",
    "jest-junit": "^5.1.0",
    "prettier": "1.14.2",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "dependencies": {
    "is-promise": "^2.1.0",
    "ow": "^0.6.0"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "tests/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
